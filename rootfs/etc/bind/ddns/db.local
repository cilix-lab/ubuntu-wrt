;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ubuntuwrt.local. admin.ubuntuwrt.local. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ubuntuwrt.local.
@	IN	A	192.168.1.1
@	IN	AAAA	::1
ubuntuwrt	IN	A	192.168.1.1
